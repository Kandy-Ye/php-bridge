{
  "name": "warps/php-bridge",
  "description": "Php bridge to projects using other languages",
  "type": "library",
  "keywords": [
    "nutch", "php-bridge"
  ],
  "homepage": "http://warpspeed.cn",
  "license": "MIT",
  "support": {
    "source": "https://github.com/galaxyeye/php-bridge"
  },
  "require": {
    "php": ">=5.3.0"
  },
  "suggest": {
    "phpunit/phpunit": "Allows automated tests to be run without system-wide install."
  },
  "autoload": {
    "psr-4": {
      "Warps\\": "src"
    },
    "files": [
      "src/Utils/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Warps\\Test\\": "tests"
    }
  }
}
